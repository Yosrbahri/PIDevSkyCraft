{% extends 'base.html.twig' %}

{% block title %}Volgenerique index{% endblock %}

{% block body %}
{{parent()}}
     <!-- ============================================================== -->
    <!-- Start right Content here -->
    <!-- ============================================================== -->
    <div class="content-page">
    <!-- Start content -->
    <div class="content">
    <div class="container-fluid">


    <div class="row">

    <div class="col-12">
    <div class="card-box">
   

    <p class="text-muted font-14 m-b-20">

    <h4 class="m-t-0 header-title">Liste vol generique </h4>
    <div class="card">
        <div class="card-body">

    <table class="table">
        <thead>
            <tr>
                
                <th>HeureDepart</th>
                <th>Heurearrivee</th>
                <th>LieuArrive</th>
                <th>LieuDepart</th>
                <th>Montant</th>
                <th>FactureId</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for volgenerique in volgeneriques %}
            <tr>
                
                <td>{{ volgenerique.heureDepart ? volgenerique.heureDepart|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ volgenerique.heurearrivee ? volgenerique.heurearrivee|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ volgenerique.lieuArrive }}</td>
                <td>{{ volgenerique.lieuDepart }}</td>
                <td>{{ volgenerique.montant }}</td>
                <td>{{ volgenerique.factureId }}</td>
                <td>
                    <a class="btn btn-info " href="{{ path('app_volgenerique_show', {'id': volgenerique.id}) }}">show</a>
                    <a class="btn btn-warning" href="{{ path('app_volgenerique_edit', {'id': volgenerique.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="8">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a class="btn btn-success" href="{{ path('app_volgenerique_new') }}">Create new</a>
{% endblock %}
